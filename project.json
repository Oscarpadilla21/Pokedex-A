{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' https://pokedex-a-alpha.vercel.app/; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://pokedex-a-alpha.vercel.app/; style-src 'self' 'unsafe-inline' https://pokedex-a-alpha.vercel.app/; font-src 'self' data:;"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://pokedex-a-alpha.vercel.app/"
        }
      ]
    }
  ]
}
